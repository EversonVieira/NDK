@inherits NDKBaseInput<TValue>
@typeparam TValue
<input id="@Id"
       name="@Name"
       title="@Title"
       class="@GetClass()"
       style="@Style"
       @attributes="@AdditionalAttributes"
       disabled="@Disabled"
       
       @ref="Element"
       @onchange="@((ChangeEventArgs args) => Set(args))"
       >

